  },
  "pre-commit": "lint-staged",
  "engines": {
    "node": ">= 14.0.1"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
