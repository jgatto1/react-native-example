{
  "name": "seeking-safety",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ./src --cache --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint ./src --cache --fix --ext js,jsx,ts,tsx",
    "type-check": "tsc --pretty --noEmit",
    "prepare": "husky install",
    "link": "react-native link",
    "bump": "npx react-native bump-version --skip-semver-for all",
    "bump:android": "npx react-native bump-version --skip-semver-for all --skip-code-for ios",
    "bump:ios": "npx react-native bump-version --skip-semver-for all --skip-code-for android",
    "start-deploy": "open ios/SeekingSafety.xcworkspace & npm run build:android",
    "build:android": "cd android/ && ./gradlew clean && ./gradlew bundleRelease && open app/build/outputs/bundle/release",
    "build:android:apk": "cd android/ && ./gradlew clean assembleRelease"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "npm run lint:fix"
    ]
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.1.5",
